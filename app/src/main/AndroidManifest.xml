<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sairaa.scholarquiz">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppInfo"
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".SignInActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".HomeActivity" />

        <activity android:name=".UserChannelActivity" />
        <activity android:name=".AllChannelListActivity" />

        <activity android:name=".MyScorecardChannelActivity" />
        <activity android:name=".MyScoreQuizListActivity" />
        <activity android:name=".MyScorecardActivity" />

        <activity android:name=".LeaderboardChannelActivity" />
        <activity android:name=".LeaderboardQuizListActivity" />
        <activity android:name=".LeaderScoreboardActivity" />

        <activity android:name=".QuizListActivity" />
        <activity android:name=".CreateQuizActivity" />
        <activity android:name=".CreateQuizQuestionActivity" />

        <activity android:name=".QuizScoreActivity" />
        <activity android:name=".QuizActivity"/>
    </application>

</manifest>