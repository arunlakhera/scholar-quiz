<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sairaa.scholarquiz">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppInfo"
        android:allowBackup="true"
        android:icon="@mipmap/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignInActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".SignUpActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".QuizListActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".QuizActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".UserChannelActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".AllChannelListActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".CreateQuizActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".CreateQuizQuestionActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".QuizScoreActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".HomeActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".MyScorecardChannelActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".MyScoreQuizListActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".MyScorecardActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".LeaderboardQuizListActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".LeaderboardChannelActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".LeaderScoreboardActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".AdminHomeActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".AdminCreateChannelActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".AdminChannelListActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".AdminUserListActivity" android:configChanges="orientation" android:screenOrientation="portrait"/>
        <activity android:name=".UserProfileActivity" android:configChanges="orientation" android:screenOrientation="portrait"></activity>
    </application>

</manifest>